{
    "status": 200,
    "features": {
        "android-app-update": {
            "defaultValue": {},
            "rules": [
                {
                    "condition": {
                        "$and": [
                            {
                                "roles": {
                                    "$elemMatch": {
                                        "$eq": "tester"
                                    }
                                }
                            },
                            {
                                "buildNumber": {
                                    "$lte": 10
                                }
                            }
                        ]
                    },
                    "force": {
                        "updateType": "ImmediateUpdate",
                        "latestVersion": "2.20.2"
                    }
                },
                {
                    "condition": {
                        "$and": [
                            {
                                "roles": {
                                    "$elemMatch": {
                                        "$eq": "user"
                                    }
                                }
                            },
                            {
                                "buildNumber": {
                                    "$lte": 10
                                }
                            }
                        ]
                    },
                    "force": {
                        "updateType": "FlexibleUpdate",
                        "latestVersion": "2.20.2"
                    }
                }
            ]
        },
        "user-theme-pref": {
            "defaultValue": "light",
            "rules": [
                {
                    "condition": {
                        "roles": {
                            "$elemMatch": {
                                "$eq": "admin"
                            }
                        }
                    },
                    "force": "dark"
                }
            ]
        }
    }
}
{
  "banner_text": {
    "defaultValue": "Welcome to Acme Donuts!",
    "rules": [
      {
        "condition": {
          "country": "france"
        },
        "force": "Bienvenue au Beignets Acme !"
      },
      {
        "condition": {
          "country": "spain"
        },
        "force": "Â¡Bienvenidos y bienvenidas a Donas Acme!"
      }
    ]
  },
  "dark_mode": {
    "defaultValue": false,
    "rules": [
      {
        "condition": {
          "loggedIn": true
        },
        "force": true,
        "coverage": 0.5,
        "hashAttribute": "id"
      }
    ]
  },
  "donut_price": {
    "defaultValue": 2.5,
    "rules": [
      {
        "condition": {
          "employee": true
        },
        "force": 0
      }
    ]
  },
  "meal_overrides_gluten_free": {
    "defaultValue": {
      "meal_type": "standard",
      "dessert": "Strawberry Cheesecake"
    },
    "rules": [
      {
        "condition": {
          "dietaryRestrictions": {
            "$elemMatch": {
              "$eq": "gluten_free"
            }
          }
        },
        "force": {
          "meal_type": "gf",
          "dessert": "French Vanilla Ice Cream"
        }
      }
    ]
  }
}
